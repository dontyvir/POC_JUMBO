#Compara resultado anterior si esta vacía la variable 

if [ "$resultado" == "" ]
then
    echo $(date) - 'voy a ejecutar el proceso captura mas purga' >> logs.txt
    cd /u02/WebSphere/AppServer/profiles/default/deamons/GatillaCapturaTBK
